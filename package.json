{
  "name": "lib-ui-vue",
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/lib-ui-vue.umd.cjs",
  "module": "./dist/lib-ui-vue.js",
  "exports": {
    ".": {
      "import": "./dist/lib-ui-vue.js",
      "require": "./dist/lib-ui-vue.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "publishConfig": {
    "lib-ui-vue:registry": "https://npm.pkg.github.com/"
  },
  "types": "./dist/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,vue}": [
      "yarn lint --fix",
      "yarn test"
    ]
  },
  "scripts": {
    "dev": "vite",
    "lint": "vue-tsc --noEmit && eslint src/**/*.{ts,vue} --max-warnings=0",
    "format": "prettier .  --write",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "types": "vue-tsc ",
    "test": "vitest --no-watch -- --bail",
    "test:watch": "vitest",
    "preview": "vite preview",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@storybook/addon-essentials": "^7.0.21",
    "@storybook/addon-interactions": "^7.0.21",
    "@storybook/addon-links": "^7.0.21",
    "@storybook/addon-styling": "^1.3.0",
    "@storybook/blocks": "^7.0.21",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/vue3": "^7.0.21",
    "@storybook/vue3-vite": "^7.0.21",
    "@testing-library/vue": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-c8": "^0.29.8",
    "@vitest/ui": "^0.32.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/test-utils": "^2.3.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-vue": "^9.10.0",
    "happy-dom": "^9.1.9",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "storybook": "^7.0.21",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.29.8",
    "vue-eslint-parser": "^9.1.1",
    "vue-tsc": "^1.2.0"
  }
}
